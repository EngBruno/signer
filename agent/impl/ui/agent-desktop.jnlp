<?xml version="1.0" encoding="utf-8"?>
<!-- JNLP File for Desktop Agent Application -->
<jnlp
  spec="6.0+"
  codebase="http://stdcs.supst.serpro/desktop-agent/"
  href="agent-desktop.jnlp">
  <information>
    <title>Demoiselle Signer Desktop</title>
    <vendor>SERPRO</vendor>
    <homepage href="index.html"/>
    <description>Demoiselle Signer Desktop Example Application</description>
    <offline-allowed/> 
    <association mime-type="application-x/da-file"  extensions="da"/> 
    <shortcut online="false" install="false">
      <desktop/>
      <menu submenu="Demoiselle Signer"/>
    </shortcut>
  </information>
  <security>
      <all-permissions/>
  </security>
  <resources>
    <j2se version="1.6+" />
    <jar href="das.jar"/>
  </resources>
  <application-desc main-class="org.demoiselle.signer.agent.desktop.Main"/>
</jnlp>
